ex_9

#include <stdio.h>
int main () {
   int x;
   int y;
   float distancia;
   printf(" digite a coordenada x:");
   scanf("%d", &x);
   printf("Digite a coordenada y:");
   scanf("%d",&y);
   printf("\n\nDistancia entre os pontos: %5.2f\n", distancia = sqrt(pow(x,2)+pow(y,2)));
  return 0 ;




